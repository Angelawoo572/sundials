SHELL = @SHELL@

srcdir       = @srcdir@
top_builddir = @top_builddir@
builddir     = @builddir@
abs_builddir = @abs_builddir@
prefix       = @prefix@
exec_prefix  = @exec_prefix@
includedir   = @includedir@
libdir       = @libdir@

INSTALL      = @INSTALL@
INSTALL_PROG = @INSTALL_PROGRAM@
INSTALL_FILE = @INSTALL_DATA@

LIBTOOL      = @LIBTOOL@
LIBTOOL_DEPS = @LIBTOOL_DEPS@

CPP      = @CPP@
CPPFLAGS = @CPPFLAGS@
CC       = @CC@
CFLAGS   = @CFLAGS@
LDFLAGS  = @LDFLAGS@
LIBS     = @LIBS@

LAPACK_ENABLED = @LAPACK_ENABLED@
BLAS_LAPACK_LIBS = @BLAS_LAPACK_LIBS@

OBJ_EXT = @OBJEXT@
EXE_EXT = @EXEEXT@

EXS_INSTDIR = @EXS_INSTDIR@

top_srcdir = $(srcdir)/../../..

SUNDIALS_INCS = -I$(top_srcdir)/include

SUNDIALS_LIBS = $(top_builddir)/src/cvodes/libsundials_cvodes.la          \
	        $(top_builddir)/src/nvec_ser/libsundials_nvecserial.la

SUNDIALS_BL_LIBS = $(top_builddir)/src/cvodes/libsundials_cvodes_bl.la    \
	           $(top_builddir)/src/cvodes/libsundials_cvodes.la       \
	           $(top_builddir)/src/nvec_ser/libsundials_nvecserial.la

EXEC_FILES = cvsdenx$(EXE_EXT) \
	     cvsdenx_uw$(EXE_EXT) \
	     cvsbanx$(EXE_EXT) \
	     cvskryx$(EXE_EXT) \
	     cvskryx_bp$(EXE_EXT) \
	     cvsdirectdem$(EXE_EXT) \
	     cvskrydem_pre$(EXE_EXT) \
	     cvsfwdnonx$(EXE_EXT) \
	     cvsfwddenx$(EXE_EXT) \
	     cvsfwdkryx$(EXE_EXT) \
	     cvsadjdenx$(EXE_EXT) \
	     cvsadjbanx$(EXE_EXT) \
	     cvsadjkryx_int$(EXE_EXT) \
	     cvsadjkryx_pnt$(EXE_EXT) \
	     cvskrydem_lin$(EXE_EXT)

EXEC_BL_FILES = cvsbanx_lap$(EXE_EXT) \
	        cvsdenx_lap$(EXE_EXT)

mkinstalldirs = $(SHELL) $(top_srcdir)/config/mkinstalldirs
rminstalldirs = $(SHELL) $(top_srcdir)/config/rminstalldirs

# ----------------------------------------------------------------------------------------------------------------------

all: $(EXEC_FILES)
	@if test "X${LAPACK_ENABLED}" = "Xyes"; then \
