# File tarpvode: Script to build PVODE tar-file, pvode.tar.
# To execute, type 'sundials/scripts/tarpvode' in the directory
# containing /sundials/.
# Version of 7 February 2002.

tar cvf pvode.tar sundials/doc/Readme

tar uvf pvode.tar sundials/lib

tar uvf pvode.tar sundials/shared/include/llnltyps.h
tar uvf pvode.tar sundials/shared/include/llnlmath.h
tar uvf pvode.tar sundials/shared/include/nvector_parallel.h
tar uvf pvode.tar sundials/shared/include/smalldense.h
tar uvf pvode.tar sundials/shared/include/band.h
tar uvf pvode.tar sundials/shared/include/spgmr.h
tar uvf pvode.tar sundials/shared/include/iterativ.h
tar uvf pvode.tar sundials/shared/include/fcmixpar.h

tar uvf pvode.tar sundials/shared/source/llnlmath.c
tar uvf pvode.tar sundials/shared/source/nvector_parallel.c
tar uvf pvode.tar sundials/shared/source/smalldense.c
tar uvf pvode.tar sundials/shared/source/band.c
tar uvf pvode.tar sundials/shared/source/spgmr.c
tar uvf pvode.tar sundials/shared/source/iterativ.c

tar uvf pvode.tar sundials/cvode/doc/pvreadme
tar uvf pvode.tar sundials/cvode/doc/pvudoc.ps
tar uvf pvode.tar sundials/cvode/doc/pvaddenda

tar uvf pvode.tar sundials/cvode/include/cvdiag.h
tar uvf pvode.tar sundials/cvode/include/cvspgmr.h
tar uvf pvode.tar sundials/cvode/include/pvbbdpre.h
tar uvf pvode.tar sundials/cvode/include/cvode.h

tar uvf pvode.tar sundials/cvode/source/Makefile
tar uvf pvode.tar sundials/cvode/source/pvode.mk
tar uvf pvode.tar sundials/cvode/source/cvdiag.c
tar uvf pvode.tar sundials/cvode/source/cvspgmr.c
tar uvf pvode.tar sundials/cvode/source/pvbbdpre.c
tar uvf pvode.tar sundials/cvode/source/cvode.c

tar uvf pvode.tar sundials/cvode/examples/Makefile
tar uvf pvode.tar sundials/cvode/examples/cvodex.mk
tar uvf pvode.tar sundials/cvode/examples/pvodex.mk
tar uvf pvode.tar sundials/cvode/examples/pvnx.c
tar uvf pvode.tar sundials/cvode/examples/pvnx.out
tar uvf pvode.tar sundials/cvode/examples/pvkx.c
tar uvf pvode.tar sundials/cvode/examples/pvkx.out
tar uvf pvode.tar sundials/cvode/examples/pvkxb.c
tar uvf pvode.tar sundials/cvode/examples/pvkxb.out

tar uvf pvode.tar sundials/cvode/fcmix/Makefile
tar uvf pvode.tar sundials/cvode/fcmix/fpvode.mk
tar uvf pvode.tar sundials/cvode/fcmix/fpvodex.mk
tar uvf pvode.tar sundials/cvode/fcmix/fpvode.h
tar uvf pvode.tar sundials/cvode/fcmix/fpvode.c
tar uvf pvode.tar sundials/cvode/fcmix/fpvspgmr01.c
tar uvf pvode.tar sundials/cvode/fcmix/fpvspgmr10.c
tar uvf pvode.tar sundials/cvode/fcmix/fpvspgmr11.c
tar uvf pvode.tar sundials/cvode/fcmix/fpvspgmr20.c
tar uvf pvode.tar sundials/cvode/fcmix/fpvspgmr21.c
tar uvf pvode.tar sundials/cvode/fcmix/fpvpreco.c
tar uvf pvode.tar sundials/cvode/fcmix/fpvpsol.c
tar uvf pvode.tar sundials/cvode/fcmix/fpvjtimes.c
tar uvf pvode.tar sundials/cvode/fcmix/fpvbbd.h
tar uvf pvode.tar sundials/cvode/fcmix/fpvbbd.c
tar uvf pvode.tar sundials/cvode/fcmix/fpvbbdin1.c
tar uvf pvode.tar sundials/cvode/fcmix/pvdiagnf.f
tar uvf pvode.tar sundials/cvode/fcmix/pvdiagnf.out
tar uvf pvode.tar sundials/cvode/fcmix/pvdiagkf.f
tar uvf pvode.tar sundials/cvode/fcmix/pvdiagkf.out
tar uvf pvode.tar sundials/cvode/fcmix/pvdiagkbf.f
tar uvf pvode.tar sundials/cvode/fcmix/pvdiagkbf.out
