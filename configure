  "shared/source/Makefile" ) CONFIG_FILES="$CONFIG_FILES shared/source/Makefile" ;;
  "nvec_ser/Makefile" ) CONFIG_FILES="$CONFIG_FILES nvec_ser/Makefile" ;;
