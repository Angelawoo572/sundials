SHELL = @SHELL@

srcdir       = @srcdir@
builddir     = @builddir@
top_builddir = @top_builddir@
prefix       = @prefix@
exec_prefix  = @exec_prefix@
includedir   = @includedir@
libdir       = @libdir@

@LIBTOOL_CMD@

CPP      = @CPP@
CPPFLAGS = @CPPFLAGS@
CC       = @CC@
CFLAGS   = @CFLAGS@
LDFLAGS  = @LDFLAGS@
LIBS     = @LIBS@

OBJ_EXT = @OBJ_EXT@

COMPILER_PREFIX = @COMPILER_PREFIX@
LINKER_PREFIX   = @LINKER_PREFIX@

SUNDIALS_INC_DIR = $(includedir)
SUNDIALS_LIB_DIR = $(libdir)
SUNDIALS_LIBS    = -lsundials_cvodes -lsundials_nvecserial -lsundials_shared

EXEC_FILES = cvsdenx cvsdenxe cvsbanx cvskryx cvskryxb cvsdirectdem cvskrydem_pre cvsfwdnonx cvsfwddenx cvsfwdkryx cvsadjdenx cvsadjbanx cvsadjkryx_int cvsadjkryx_intb cvskryxt

all: $(EXEC_FILES)

cvsdenx: $(srcdir)/cvsdenx.c
	$(COMPILER_PREFIX) $(CC) $(CPPFLAGS) -I$(SUNDIALS_INC_DIR) $(CFLAGS) -c $(srcdir)/cvsdenx.c
	$(LINKER_PREFIX) $(CC) -o $(builddir)/cvsdenx $(builddir)/cvsdenx.$(OBJ_EXT) $(LDFLAGS) -L$(SUNDIALS_LIB_DIR) $(SUNDIALS_LIBS) $(LIBS)
